{
  "name": "welldone-bridge-hackatom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "gcp-predeploy": "find ./.next -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./.next -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "deploy": "rm -rf ./.next && NODE_ENV=production next build && npm run gcp-predeploy && gcloud app deploy"  
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/system": "^5.1.0",
    "jwt-decode": "^3.1.2",
    "next": "11.1.2",
    "next-i18next": "^8.8.0",
    "react": "17.0.2",
    "react-blockies": "^1.4.1",
    "react-dom": "17.0.2",
    "react-lazy-load-image-component": "^1.5.1",
    "sass": "^1.41.0",
    "slick-carousel": "^1.8.1",
    "typescript": "4.4.2",
    "web3": "^1.6.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.1.2",
    "@types/react": "17.0.19",
    "@types/react-blockies": "^1.4.1",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "prettier": "^2.3.2"
  }
}
